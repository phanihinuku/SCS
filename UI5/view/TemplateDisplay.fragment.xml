<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<l:Grid
		defaultSpan="L12 M12 S12"
		width="auto">
		<l:content>
			<f:SimpleForm id="SimpleFormDisplay354D"
				minWidth="1024"
				maxContainerCols="2"
				editable="false"
				layout="ResponsiveGridLayout"
				title="Name:{TM>template_name}"
				labelSpanL="3"
				labelSpanM="3"
				emptySpanL="4"
				emptySpanM="4"
				columnsL="1"
				columnsM="1">
				<f:content>
					<Label text="TemplateId" />
					<Text text="{TM>templateid}" id="templateIdv" />
					<Label text="Template Name" />
					<Text text="{TM>template_name}" id="templateNamev" />
					<Label text="Template " />
					<Text text="{TM>template_text}" id="templateTextv" />
				
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>